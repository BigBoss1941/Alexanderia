<script>
    const main_page = document.querySelector(".hdm");
    document.addEventListener('scroll', () => {
        if (window.scrollY === 0){
            if(main_page.classList.contains("front")){
                console.log("nice")
            }
            else {
                main_page.classList.add("front")
                document.querySelector("#sbar").classList.add("front")
            }
        }
        else if (window.scrollY > 0){
            main_page.classList.remove("front");
            document.querySelector("#sbar").classList.remove("front")
        }

    })
    console.log('hi');

</script>


